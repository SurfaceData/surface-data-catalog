{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": ">=14.19 <=16.x",
    "yarn": ">=1.15"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "scripts": {
    "extract-translations": "i18next --fail-on-warnings",
    "setup-admin": "yarn rw exec setup_admin"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.87.0",
    "@aws-sdk/s3-request-presigner": "^3.100.0",
    "command-line-args": "^5.2.1",
    "generate-api-key": "^1.0.0",
    "i18next-parser": "^6.4.0",
    "react-code-blocks": "^0.0.9-0",
    "react-icons": "^4.3.1",
    "react-markdown": "^8.0.3",
    "react-responsive": "^9.0.0-beta.10",
    "rsuite": "^5.11.0",
    "simple-git": "^3.9.0",
    "styled-components": "^5.3.5",
    "template-file": "^6.0.1",
    "toml": "^3.0.0"
  },
  "resolutions": {
    "cross-undici-fetch": "^0.4.5",
    "dset": "^3.1.2",
    "glob-parent": "^5.1.2",
    "got": "^11.8.5",
    "highlight.js": "^10.4.1",
    "prismjs": "^1.27.0",
    "react-syntax-highlighter": "^15.5.0",
    "trim": "^0.0.3",
    "trim-newlines": "^3.0.1",
    "undici": "^5.5.1"
  }
}
