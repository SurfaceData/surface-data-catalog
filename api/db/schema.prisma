datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model UserRoles {
  id    String @id
  role  String
}

model Dataset {
  id       String @id
  name     String
  language String
  task     String
  path     String
  license  String
}
